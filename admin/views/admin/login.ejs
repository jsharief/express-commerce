<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/forms.css">
    <link rel="stylesheet" href="/css/login.css">
</head>

<body>
   <%- include('../includes/navigation.ejs') %>

    <main>
        <form class="login-form" action="/agent/processLogin" method="POST">
            <div class="form-control">

                <label for="username">Admin username</label>
                <input type="text" name="username" id="username"  placeholder="admin@expresscommerce.com">

            </div>

            <div class="form-control">

                <label for="passowrd">Admin password</label>
                <input type="password" name="passowrd" id="passowrd"  placeholder="***************">

            </div>   
            
            <button class="btn" type="submit">Login</button>
        </form>
    </main>

    <%- include('../includes/end.ejs') %>
